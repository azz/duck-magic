{
  "name": "duck-magic",
  "version": "0.0.0-development",
  "description": "Utilities for Ducks - Modular Redux",
  "repository": "azz/duck-magic",
  "author": "Lucas Azzola <@azz>",
  "keywords": [
    "redux",
    "ducks",
    "modular"
  ],
  "main": "./dist",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "redux": "^4.0.1"
  },
  "scripts": {
    "build": "babel src -d dist --copy-files --ignore __tests__",
    "test": "jest",
    "lint": "eslint . --ignore-path=.gitignore",
    "precommit": "pretty-quick --staged"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "prettier": "~1.14.3",
    "pretty-quick": "^1.8.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "2018"
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
